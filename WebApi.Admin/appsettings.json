    "Jwt": {
      "Key": "!123",
      "Issuer": "MyIssuer",
      "Audience": "MyAudience"
    },
    "Password": {
      "GlobalSalt": "salt"
    }
  },
  "FileUpload": {
    "Folder": "Resources/Books",
    "HostAddress": "http://localhost:5002/"
  }
}
