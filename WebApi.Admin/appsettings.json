{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "User ID=postgres;Password=...;Server=localhost;Port=5432;Database=AXLBookCatalog;Integrated Security=true;Pooling=true;"
  },
  "AllowedHosts": "*",
  "Authentication": {
    "TimeoutAccessTokenMinutes": "43200",
